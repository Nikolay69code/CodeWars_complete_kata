package warrior;
// Задания взяты с сайта https://postupi.online/filesolym_standart.php?id=8369&is_preview=1
public class Olimpiada {

	//Задача 1. Самолёт
	public static int plane (int seats) {
		if (seats > 10) {	
			int otv = 2;
			int seatsW = seats - 6;
			otv += seatsW / 4 - 1;
			if (seatsW / 4 * 4 != seatsW ) {
				otv++;
				return otv;
			}
		}
		else {
			if (seats < 7) {
				return 1;
			}
			if (seats < 10) {
				return 2;
			}
		}
		return 0;
	}
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.out.print(plane(10));
	}

}
